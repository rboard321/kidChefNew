{
  "name": "kidchef",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start:dev": "NODE_ENV=development EXPO_PUBLIC_ENVIRONMENT=development expo start --clear",
    "start:staging": "NODE_ENV=staging EXPO_PUBLIC_ENVIRONMENT=staging expo start --clear",
    "start:prod": "NODE_ENV=production EXPO_PUBLIC_ENVIRONMENT=production expo start --no-dev --minify --clear",
    "build:dev": "EXPO_PUBLIC_ENVIRONMENT=development eas build --platform all --profile development",
    "build:staging": "EXPO_PUBLIC_ENVIRONMENT=staging eas build --platform all --profile preview",
    "build:prod": "EXPO_PUBLIC_ENVIRONMENT=production eas build --platform all --profile production",
    "test": "vitest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/material-top-tabs": "^7.4.11",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "@stripe/stripe-react-native": "0.50.3",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-persist-client": "^5.90.14",
    "expo": "~54.0.30",
    "expo-application": "^7.0.8",
    "expo-av": "^16.0.8",
    "expo-crypto": "~15.0.8",
    "expo-device": "^8.0.10",
    "expo-image": "^3.0.11",
    "expo-linking": "^8.0.11",
    "expo-network": "^8.0.8",
    "expo-sensors": "^15.0.8",
    "expo-speech": "^14.0.8",
    "expo-status-bar": "~3.0.9",
    "firebase": "^12.7.0",
    "buffer": "^6.0.3",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@expo/cli": "^54.0.20",
    "@expo/ngrok": "^4.1.3",
    "@react-native-community/cli-server-api": "^20.0.2",
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.72.8",
    "firebase-admin": "^13.6.0",
    "typescript": "~5.9.2",
    "vitest": "^1.6.0"
  },
  "private": true
}
